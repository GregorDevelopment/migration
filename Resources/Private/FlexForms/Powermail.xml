<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3FlexForms>
	<data>
		<sheet index="main">
			<language index="lDEF">
				<field index="settings.flexform.main.form">
					<value index="vDEF">{helper.formUid}</value>
				</field>
				<field index="settings.flexform.main.confirmation">
					<value index="vDEF">{helper.confirmationPage}</value>
				</field>
				<field index="settings.flexform.main.pid">
					<value index="vDEF"></value>
				</field>
			</language>
		</sheet>
		<sheet index="receiver">
			<language index="lDEF">
				<field index="settings.flexform.receiver.type">
					<value index="vDEF">0</value>
				</field>
				<field index="settings.flexform.receiver.name">
					<value index="vDEF">Empfänger Name</value>
				</field>
				<field index="settings.flexform.receiver.email">
					<value index="vDEF">{helper.receiverEmail}</value>
				</field>
				<field index="settings.flexform.receiver.subject">
					<value index="vDEF">{helper.subject}</value>
				</field>
				<field index="settings.flexform.receiver.body">
					<value index="vDEF">&lt;p&gt;{powermail_all<f:comment></f:comment>}&lt;/p&gt;</value>
				</field>
			</language>
		</sheet>
		<sheet index="sender">
			<language index="lDEF">
				<field index="settings.flexform.sender.name">
					<value index="vDEF">Universität Tübingen</value>
				</field>
				<field index="settings.flexform.sender.email">
					<value index="vDEF">{helper.receiverEmail}</value>
				</field>
				<field index="settings.flexform.sender.subject">
					<value index="vDEF"></value>
				</field>
				<field index="settings.flexform.sender.body">
					<value index="vDEF">&lt;p&gt;{powermail_all<f:comment></f:comment>}&lt;/p&gt;</value>
				</field>
			</language>
		</sheet>
		<sheet index="thx">
			<language index="lDEF">
				<field index="settings.flexform.thx.body">
					<value index="vDEF">&lt;p&gt;{powermail_all<f:comment></f:comment>}&lt;/p&gt;</value>
				</field>
				<field index="settings.flexform.thx.redirect">
					<value index="vDEF">{helper.redirectPid}</value>
				</field>
			</language>
		</sheet>
	</data>
</T3FlexForms>
